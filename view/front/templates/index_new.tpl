<div class="main-area">
	
	<div class="scroll-area">
		<div class="port">
			<a href="" title="" target="_blank" ></a>
		</div>
		<h2>淘都推荐</h2>
		<div id="one-carousel" class="tang-ui tang-carousel">
		<a class="tang-carousel-btn tang-carousel-btn-prev" href="javascript:void(0)"></a>
		<div class="tang-carousel-container">
		<ul class="tang-carousel-element" style="width: 3860px;">
			{foreach from=$recommend item=r}
			<li class="tang-carousel-item goods-item">
			<div class="icon-area">
				<div class="icon-wrap icon-reservation"></div>
			</div>
			<a class="img-wrap" target="_blank" href="http://item.taobao.com/item.htm?id={$r.iid}">
				<img class="img-recommend-pic" src="{$r.pic}" width="220" height="220" alt="{$r.title}">
				<div class="img-border"></div>
			</a>
			<div class="info">
				<div class="mask"></div>
				<a target="_blank" href="http://item.taobao.com/item.htm?id={$r.iid}" title="{$r.title}" >{$r.title}</a>
			</div>
			<a class="control-bar" href="http://item.taobao.com/item.htm?id={$r.iid}" target="_blank">
			<span class="bought-wrap">{$r.volume}人购买</span>
			<strong>￥<em>{$r.nprice}</em></strong> <i>￥{$r.oprice}</i>
			</a>
			</li>
			{/foreach}
			{foreach from=$recommend item=r key=k}
			{if $k <= 3}
			<li class="tang-carousel-item goods-item">
			<div class="icon-area">
				<div class="icon-wrap icon-reservation"></div>
			</div>
			<a class="img-wrap" target="_blank" href="http://item.taobao.com/item.htm?id={$r.iid}">
				<img class="img-recommend-pic" src="{$r.pic}" width="220" height="220" alt="{$r.title}">
				<div class="img-border"></div>
			</a>
			<div class="info">
				<div class="mask"></div>
				<a target="_blank" href="http://item.taobao.com/item.htm?id={$r.iid}" title="{$r.title}" >{$r.title}</a>
			</div>
			<a class="control-bar" href="http://item.taobao.com/item.htm?id={$r.iid}" target="_blank">
			<span class="bought-wrap">{$r.volume}人购买</span>
			<strong>￥<em>{$r.nprice}</em></strong> <i>￥{$r.oprice}</i>
			</a>
			</li>
			{/if}
			{/foreach}
		</ul>
		</div>
		<a class="tang-carousel-btn tang-carousel-btn-next" href="javascript:void(0)" id="TANGRAM_25__next"></a>
		</div>
	</div>

	<div class="goods-area">
		<div class="control-area" id="ControlArea">
			<div class="control-wrap" id="ControlWrap">
				<div class="control-left-wrap"></div>
				<div class="control-center-wrap">
					<div class="control-banner clearfix" mon="type=indexfilter">
						<div class="title-wrap">精选商品<span style="font-size:12px;">每日十点更新</span></div>
						<div class="filter-container">
							<div class="filter-area" id="FilterArea">
								<a data-filter="9" class="filter" > 9.9包邮 </a>
								<a data-filter="39" class="filter" > 39封顶</a>
								<span class="credits-icon"></span>
								<a data-filter="8" class="filter"> 今日新单 </a>
								<a data-filter="15" class="filter">爆款1-5折</a>
							</div>
						</div>
						<div class="sort-container">
							<div class="sort-area" id="SortArea">
								<a class="sort-type default selected" id="SortDefault" title="点击按默认排序" data-order="0" >默认</a>
								<a class="sort-type sales" id="SortDes" title="点击按购买人数降序排序" data-order="4" > 销量 </a>
								<a class="sort-type price" id="Price" rel="nofollow" title="点击按价格升序排序" data-order="1"> 价格 </a>
								<a class="sort-type discount" id="SortAsc" title="点击按折扣升序排序" data-order="3" >折扣</a>
							</div>
						</div>
					</div>
					<div class="subcatg-area clearfix" id="SubcatgArea">
						<div class="subcatg-wrap first-wrap">
							<a href="/d/index?nocache=1" class="absorb-current" >全部</a>
						</div>
						{foreach from=$catList item=cat}
						<div class="subcatg-wrap">
							<a class="choose-cate" data-cate="{$cat.rank}"> {$cat.name}&nbsp;<span>{$cat.num}</span> </a>
						</div>
						{/foreach}
					</div>
				</div>
				<div class="control-right-wrap"></div>
			</div>
		</div>
		<div class="goods-container clearfix" mon="type=item" id="GoodsContainer">
		{foreach from=$product item=p key=i}
			<div class="mb20 wt-good-item-n {if $i%4==0}wt-good-first-n{else}mr19{/if}">
				<a href="http://item.taobao.com/item.htm?id={$p.iid}" class="good-item-n-wrap" target="_blank"><div class="good-item-n-border"></div></a>
				<div class="good-n-meta clearfix">
					<div class="good-n-img">
						<div class="good-indication"><i class="free-appointment"></i></div>
						<a href="http://item.taobao.com/item.htm?id={$p.iid}" style="display: block" target="_blank" ><img class="img-goods-pic" alt="$p.remark" src="/assets/images/loadimg.gif" data-ks-lazyload="{$p.pic}" ></a>
						<a class="good-loc" href="http://item.taobao.com/item.htm?id={$p.iid}" target="_blank" >
							<div class="good-loc-bg"></div>
							<span>{$p.remark|default:"不错，便宜实惠，又好看！"}</span>
						</a>
					</div>
					<div class="good-n-title">
						<a class="fcb" href="http://item.taobao.com/item.htm?id={$p.iid}" target="_blank" title="{$p.title}">{$p.title}</a>
					</div>
				</div>
				<div class="good-n-price">
					<div class="good-value"> <small>￥</small>{$p.nprice}</div>
					<div class="good-pre"> <span>￥{$p.oprice}</span> </div>
					<span class="good-rebate">{$p.discount}折</span>
				</div>
				<div class="good-n-foot">
					<a class="n-sitename" href="" target="_blank" title="{$p.seller_nick}">{$p.seller_nick}</a>
					<div class="n-bought"> {$p.volume}人购买 </div>
					<a class="favor favor-ico" href="javascript:void(0);" data-id="{$p.id}">收藏</a>
				</div>
				<div class="msg clearfix">
					<a class="close-ico" href="javscript:void(0);"></a>
					<b>收藏成功!</b>
					<a href="javascript:void(0)" class="favor-login">登录</a> 后收藏才能长久保存 
				</div>
			</div>
		{/foreach}
		</div>

		<div class="more-link-wrap" id="MoreLinkWrap" style="display: block;">
			<a href="javascript:void(0)" class="more-link" id="MoreLink" data-page="1" >
			<span class="more-tip" id="MoreTip">显示更多给力商品</span>
			<span class="ie6-vertical arrow-tip" id="ArrowTip"></span>
			</a>
		</div>
	</div>

	<div id="ad-banner-bottom" class="ad-banner-bottom">
		<div class="banner-item">
			<a href="" target="_blank" title=""><img src="http://img04.taobaocdn.com/imgextra/i4/189209818/T2dRupXuFaXXXXXXXX-189209818.jpg" width="980" height="90" alt=""><div class="img-border"></div></a>
		</div>
	</div>
	
	<div class="flink-area">
		<div class="link-head"><h2>淘都小伙伴</h2><a href="/d/link" target="_blank">友链申请</a></div>
		<ul class="big">
			{foreach from=$bigLinks item=blink key=i}
			<li {if $i%7==0}class="first"{/if}>
				<a href="{$blink.link}" target="_blank">
					<img src="{$blink.big_logo}" style="border:0px;">
				</a>
			</li>
			{/foreach}
		</ul>
	</div>
</div>
</div>
